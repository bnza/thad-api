<?xml version="1.0" ?>

<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping
                          https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">

    <entity name="App\Entity\StratigraphicRelationship" table="stratigraphic_relationships">
        <id name="id" type="integer">
            <generator/>
        </id>
        <many-to-one field="sxSU" target-entity="SU" inversed-by="relations">
            <join-column name="sx_su_id" />
        </many-to-one>
        <many-to-one field="dxSU" target-entity="SU" inversed-by="inverseRelations">
            <join-column name="dx_su_id" />
        </many-to-one>
        <one-to-one field="relationship" target-entity="App\Entity\Vocabulary\SU\Relationship">
            <join-column name="relationship_id" />
        </one-to-one>
        <unique-constraints>
            <unique-constraint columns="sx_su_id,relationship_id,dx_su_id" />
        </unique-constraints>
    </entity>
</doctrine-mapping>
